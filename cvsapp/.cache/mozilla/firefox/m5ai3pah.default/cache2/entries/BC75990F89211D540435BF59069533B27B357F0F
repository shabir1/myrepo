    <script type="text/javascript">
        $(document).ready(function () {
            //Display the welcome ad
            if ('True')
                displayWelcomeAd('False');

            var isDisplay = 'False';
            var delay = '0';
            var subscriptionPopupUrl = 'http://www.techopedia.com/subscription/msubscribe.aspx?utm_nooverride=1';
            var popupCookieName = 'TECHPS';

            /*Display the subscription popup only when the interstitial ad is not prompted to the user
            A timeout of 500ms ensures the check for $('#internads') element is done correctly */
            setTimeout(function () {
                if (!$('#internads').is(":visible"))
                    displayNewletterSubscriptionPopup(isDisplay, delay, subscriptionPopupUrl, popupCookieName);
            }, 500);
        });
    </script>
