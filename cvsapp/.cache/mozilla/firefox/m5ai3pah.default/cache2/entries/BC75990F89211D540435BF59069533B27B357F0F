    <script type="text/javascript">
        $(document).ready(function () {
            ouibounce(document.getElementById('subscriptionContainer'), {
                delay: 200,
                timer: 0,
                cookieExpire: 3,
                aggressive: false,
                callback: function () {
                    addPlaceHolderForIE($('#email'), "Email Address");
                    addPlaceHolderForIE($('#firstname'), "First Name");
                    addPlaceHolderForIE($('#lastname'), "Last Name");
                    $('.errorMsg').hide();
                    $('#subscriptionContainer').modal();
                }
            });
            $(document).on('click', '#subscriptionContainer input[type="button"]', function (event) {
                var email = $('#email').val();
                if (email == '' || !validateEmailAddress(email)) {
                    $('.errorMsg').show();
                    return false;
                }
                $('.errorMsg').hide();
                var accountInfo = {
                    emailId: btoa($('#email').val().trim()),
                    firstName: $('#firstname').val().trim(),
                    lastName: $('#lastname').val().trim()
                };
                var result = getAjaxData('/service/service.svc/subscribenewsletters', "POST", JSON.stringify(accountInfo));
                $('#subscriptionContainer').hide();
                $('#subscriptionThanks').modal();
                result.done(function () {
                    ga('send', 'event', 'email', 'subscribe-tod', 'popup');
                });
            });
            $(document).on('click', '#subscriptionThanks input[type="button"]', function () {
                $('#subscriptionThanks').hide();
                ga('send', 'event', 'email', 'subscribe-tod', 'signup');
            });

        });
    </script>
    <!--   Exit Pop-up end -->

