        //Add the google universal analytics Keyword rank tracker code
        if (document.referrer.match(/google.[a-z.]{2,10}/gi) && document.referrer.match(/cd/gi)) {
            var myString = document.referrer;
            var r = myString.match(/cd=(.*?)&/);
            var rank = parseInt(r[1]);
            var kw = myString.match(/q=(.*?)&/);

            if (kw[1].length > 0) {
