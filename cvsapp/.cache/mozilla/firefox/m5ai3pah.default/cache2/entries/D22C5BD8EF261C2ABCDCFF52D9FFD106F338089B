            A timeout of 500ms ensures the check for $('#internads') element is done correctly */
            setTimeout(function () {
                if (!$('#internads').is(":visible"))
                    displayNewletterSubscriptionPopup(isDisplay, delay, subscriptionPopupUrl, popupCookieName);
            }, 500);
        });
    </script>
